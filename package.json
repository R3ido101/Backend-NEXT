{
  "name": "@atlauncher/backend-next",
  "version": "0.1.0",
  "description": "Backend API server for ATLauncher NEXT.",
  "main": "dist",
  "scripts": {
    "build": "babel src -D -d dist",
    "clean": "rimraf ./node_modules && npm install",
    "dev": "cross-env NODE_ENV=development nodemon -w src -e js,ejs,json --exec \"babel-node src/server.js\"",
    "dev:debug": "cross-env NODE_ENV=development DEBUG=* nodemon -w src -e js,ejs,json --exec \"babel-node src/server.js\"",
    "lint": "eslint src config test migrations knexfile.js",
    "lint:watch": "esw src config test migrations knexfile.js --watch",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "precommit": "npm run lint && npm run test",
    "seed": "knex seed:run",
    "start": "node dist/server.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:ci": "cross-env NODE_ENV=test jest --runInBand",
    "test:clean": "rimraf ./db/test.sqlite3",
    "test:watch": "cross-env NODE_ENV=test jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ATLauncher/Backend-NEXT.git"
  },
  "author": {
    "name": "Ryan Dowling",
    "email": "ryan.dowling@atlauncher.com",
    "url": "https://github.com/RyanTheAllmighty"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ATLauncher/Backend-NEXT/issues"
  },
  "jest": {
    "bail": true,
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)(spec).js?(x)"
    ]
  },
  "homepage": "https://github.com/ATLauncher/Backend-NEXT#readme",
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "cache-manager": "^2.4.0",
    "cache-manager-fs": "^1.0.6",
    "compression": "^1.6.2",
    "config": "~1.26.1",
    "connect-ensure-login": "^0.1.1",
    "connect-flash": "^0.1.1",
    "connect-session-knex": "^1.3.4",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "date-fns": "^1.28.5",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "express-rate-limit": "^2.8.0",
    "express-request-id": "^1.4.0",
    "express-request-version": "^2.1.1",
    "express-session": "^1.15.3",
    "helmet": "^3.6.1",
    "highlight.js": "^9.12.0",
    "http-status": "^1.0.1",
    "knex": "^0.13.0",
    "markdown-toc": "^1.1.0",
    "merge-deep": "^3.0.0",
    "mysql": "^2.13.0",
    "mysql2": "^1.3.5",
    "oauth2-server": "^3.0.0-b4",
    "oauth2orize": "^1.8.0",
    "objection": "^0.8.4",
    "passport": "^0.3.2",
    "passport-anonymous": "^1.0.1",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "passport-oauth2-client-password": "^0.1.2",
    "pug": "^2.0.0-rc.2",
    "remarkable": "^1.7.1",
    "remarkable-meta": "^1.0.1",
    "resource-router-middleware": "^0.7.0",
    "response-time": "^2.3.2",
    "uuid": "^3.1.0",
    "vantage": "^1.7.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@atlauncher/babel-preset-atlauncher": "^0.1.0-11",
    "@atlauncher/eslint-config-atlauncher": "^0.1.0-12",
    "@atlauncher/eslint-plugin-atlauncher": "^0.2.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-polyfill": "^6.23.0",
    "babel-register": "^6.24.1",
    "cross-env": "^5.0.1",
    "eslint": "^4.1.1",
    "eslint-plugin-eslint-comments": "^1.0.2",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-new-with-error": "^1.1.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-watch": "^3.1.2",
    "faker": "^4.1.0",
    "get-port-sync": "~1.0.0",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "knex-cleaner": "^1.1.3",
    "nodemon": "^1.11.0",
    "randexp": "^0.4.5",
    "rimraf": "^2.6.1",
    "sqlite3": "^3.1.8",
    "supertest": "^3.0.0"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 8"
  }
}
