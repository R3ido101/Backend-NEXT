{
  "name": "@atlauncher/backend-next",
  "version": "0.1.0-alpha.1",
  "description": "Backend API server for ATLauncher NEXT.",
  "main": "dist",
  "scripts": {
    "clean": "rimraf ./node_modules && npm install",
    "dev": "cross-env NODE_ENV=development nodemon -w src -e js,ejs,json --exec \"babel-node src/server.js --presets es2015,stage-0\"",
    "dev:debug": "cross-env NODE_ENV=development DEBUG=* nodemon -w src -e js,ejs,json --exec \"babel-node src/server.js --presets es2015,stage-0\"",
    "build": "babel src -D -d dist --presets es2015,stage-0",
    "start": "node dist/server.js",
    "prestart": "npm run -s build",
    "lint": "eslint cli src config test migrations knexfile.js",
    "lint:watch": "esw cli src config test migrations knexfile.js --watch",
    "test": "cross-env NODE_ENV=test mocha --opts ./mocha.opts",
    "test:watch": "cross-env NODE_ENV=test mocha --opts ./mocha.opts --watch",
    "test:clean": "rimraf ./db/test.sqlite3",
    "seed": "knex seed:run",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "cli": "npm run cli:dev",
    "cli:dev": "cross-env NODE_ENV=development babel-node ./cli --presets es2015,stage-0",
    "cli:test": "cross-env NODE_ENV=test babel-node ./cli --presets es2015,stage-0",
    "cli:production": "cross-env NODE_ENV=production babel-node ./cli --presets es2015,stage-0"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ATLauncher/Backend-NEXT.git"
  },
  "author": {
    "name": "Ryan Dowling",
    "email": "ryan.dowling@atlauncher.com",
    "url": "https://github.com/RyanTheAllmighty"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ATLauncher/Backend-NEXT/issues"
  },
  "homepage": "https://github.com/ATLauncher/Backend-NEXT#readme",
  "dependencies": {
    "babel-runtime": "~6.23.0",
    "bcryptjs": "~2.4.3",
    "body-parser": "~1.16.1",
    "cache-manager": "~2.4.0",
    "cache-manager-fs": "~1.0.6",
    "compression": "~1.6.2",
    "connect-ensure-login": "~0.1.1",
    "connect-flash": "~0.1.1",
    "connect-session-knex": "~1.3.4",
    "cookie-parser": "~1.4.3",
    "cors": "~2.8.1",
    "date-fns": "~1.27.2",
    "ejs": "~2.5.6",
    "express": "~4.14.1",
    "express-rate-limit": "~2.6.0",
    "express-request-id": "~1.3.0",
    "express-request-version": "~2.0.1",
    "express-session": "~1.15.1",
    "helmet": "~3.4.0",
    "highlight.js": "~9.9.0",
    "http-status": "~1.0.1",
    "knex": "0.12.6",
    "markdown-toc": "~1.1.0",
    "morgan": "~1.8.1",
    "mysql": "~2.13.0",
    "mysql2": "~1.2.0",
    "oauth2-server": "~3.0.0-b2",
    "oauth2orize": "~1.7.0",
    "objection": "~0.6.2",
    "passport": "~0.3.2",
    "passport-anonymous": "~1.0.1",
    "passport-http": "~0.3.0",
    "passport-http-bearer": "~1.0.1",
    "passport-local": "~1.0.0",
    "passport-oauth2-client-password": "~0.1.2",
    "pug": "~2.0.0-beta9",
    "remarkable": "~1.7.1",
    "remarkable-meta": "~1.0.1",
    "resource-router-middleware": "~0.6.0",
    "response-time": "~2.3.2",
    "sqlite3": "~3.1.8",
    "uuid": "~3.0.1",
    "vantage": "~1.7.0"
  },
  "devDependencies": {
    "babel-cli": "~6.23.0",
    "babel-core": "~6.23.1",
    "babel-eslint": "~7.1.1",
    "babel-plugin-transform-class-properties": "~6.23.0",
    "babel-plugin-transform-function-bind": "~6.22.0",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-polyfill": "~6.23.0",
    "babel-preset-es2015": "~6.22.0",
    "babel-preset-stage-0": "~6.22.0",
    "babel-register": "~6.23.0",
    "chai": "~3.5.0",
    "chai-as-promised": "~6.0.0",
    "chai-http": "~3.0.0",
    "chai-shallow-deep-equal": "~1.4.4",
    "cross-env": "~3.1.4",
    "eslint": "~3.15.0",
    "eslint-config-airbnb": "~14.1.0",
    "eslint-watch": "~3.0.0",
    "faker": "~3.1.0",
    "knex-cleaner": "~1.1.3",
    "mocha": "~3.2.0",
    "nodemon": "~1.11.0",
    "pre-commit": "~1.2.2",
    "randexp": "~0.4.4",
    "rimraf": "~2.6.0"
  }
}
