{
    "extends": [
        "@atlauncher/eslint-config-atlauncher"
    ],
    "env": {
        "es6": true,
        "jest": true,
        "node": true
    },
    "rules": {
        "camelcase": [
            "error",
            {
                "properties": "never"
            }
        ],
        "new-cap": [
            "error",
            {
                "capIsNewExceptions": [
                    "Router"
                ]
            }
        ]
    }
}